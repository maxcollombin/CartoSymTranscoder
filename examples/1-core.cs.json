{
    "metadata": {
        "title": "Styling a land use layer",
        "abstract": "Styling land use data with Style & Symbology Core"
    },
    "stylingRules": [
        {
            "selector":
            {
               "op": "and",
               "args": [
                  { "op": "=", "args": [ { "sysId": "dataLayer.id" }, "Landuse" ] },
                  { "op": "=", "args": [ { "sysId": "dataLayer.type" }, "vector" ] }
               ]
            },
            "symbolizer": {
                "visibility": "false"
            },
            "nestedRules": [
                {
                    "selector": [
                        "[viz.sd < 200000 and viz.date > DATE('2020-01-01')]"
                    ],
                    "symbolizer": {
                        "visibility": "true",
                        "opacity": "0.5",
                        "zOrder": "1"
                    }
                }
            ]
        }
    ]
}